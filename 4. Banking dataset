-- Ensure schema exists and use it
CREATE SCHEMA IF NOT EXISTS banking;
SET search_path = banking;

-- Banking Dataset (PostgreSQL-compatible)
-- Tables: branches, customers, accounts, transactions
-- Purpose: Practice joins, aggregations, window functions, time-series logic, fraud/rules queries.

DROP TABLE IF EXISTS transactions;
DROP TABLE IF EXISTS accounts;
DROP TABLE IF EXISTS customers;
DROP TABLE IF EXISTS branches;

CREATE TABLE branches (
  branch_id SERIAL PRIMARY KEY,
  name VARCHAR(100) NOT NULL,
  city VARCHAR(80) NOT NULL
);

CREATE TABLE customers (
  customer_id SERIAL PRIMARY KEY,
  full_name VARCHAR(120) NOT NULL,
  email VARCHAR(150) UNIQUE NOT NULL,
  city VARCHAR(80) NOT NULL,
  dob DATE NOT NULL,
  signup_date DATE NOT NULL
);

CREATE TABLE accounts (
  account_id SERIAL PRIMARY KEY,
  customer_id INT NOT NULL REFERENCES customers(customer_id),
  branch_id INT NOT NULL REFERENCES branches(branch_id),
  account_type VARCHAR(20) NOT NULL CHECK (account_type IN ('checking','savings','credit')),
  opened_date DATE NOT NULL,
  status VARCHAR(20) NOT NULL CHECK (status IN ('active','frozen','closed')),
  credit_limit NUMERIC(12,2),
  current_balance NUMERIC(14,2) NOT NULL DEFAULT 0
);

CREATE TABLE transactions (
  txn_id SERIAL PRIMARY KEY,
  account_id INT NOT NULL REFERENCES accounts(account_id),
  txn_timestamp TIMESTAMP NOT NULL,
  txn_type VARCHAR(20) NOT NULL CHECK (txn_type IN ('deposit','withdrawal','payment','transfer_in','transfer_out','fee','interest')),
  amount NUMERIC(14,2) NOT NULL CHECK (amount > 0),
  channel VARCHAR(20) NOT NULL CHECK (channel IN ('atm','branch','online','mobile','pos')),
  description VARCHAR(200)
);

-- Useful indexes
CREATE INDEX idx_accounts_customer ON accounts(customer_id);
CREATE INDEX idx_txn_account_ts ON transactions(account_id, txn_timestamp);

